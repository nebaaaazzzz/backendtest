{
  "name": "nodsetupwitswc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/bot.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/bot.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grammyjs/conversations": "^1.1.1",
    "@grammyjs/i18n": "^1.0.1",
    "@grammyjs/menu": "^1.2.0",
    "@grammyjs/ratelimiter": "^1.1.6",
    "@grammyjs/router": "^2.0.0",
    "@grammyjs/storage-typeorm": "^2.2.0",
    "dotenv": "^16.0.3",
    "grammy": "^1.15.3",
    "mysql2": "^3.2.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^3.0.1",
    "typescript": "*"
  }
}
